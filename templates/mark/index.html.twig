
    <h1>Mark index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for mark in marks %}
            <tr>
                <td>{{ mark.id }}</td>
                <td>{{ mark.name }}</td>
                <td>
                    <a href="{{ path('mark_show', {'id': mark.id}) }}">show</a>
                    <a href="{{ path('mark_edit', {'id': mark.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('mark_new') }}">Create new</a>

